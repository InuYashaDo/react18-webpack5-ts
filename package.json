  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --config .eslintrc.js"
    ],
    "*.{less,scss,css}": [
      "stylelitn --config .stylelintrc.js"
    ],
    "*.{ts,tsx,js,jsx,less,scss,css,md,json,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "conventional-changelog-cli": "^2.2.2",
